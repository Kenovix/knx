{% extends "::admin_layout.html.twig" %}

{% block menu %}
	{{ knp_menu_render('FarmaciaBundle:Builder:farmaciaMenu') }}
{% endblock %}

{% block rastro %}{{ wo_render_breadcrumbs() }}{% endblock %}

{% block titulo %}Información de la Imv_Pyp{% endblock %}

{% block msg %}
	{% if app.session.flash('ok') %}
	    <div class="alert alert-success">
	    	<button data-dismiss="alert" class="close" type="button">×</button>
	        <span>{{ app.session.flash('ok') }}</span>
	    </div>
	{% elseif app.session.flash('info') %}
		<div class="alert alert-info">
	    	<button data-dismiss="alert" class="close" type="button">×</button>
	        <span>{{ app.session.flash('info') }}</span>
	    </div>
	{% elseif app.session.flash('error') %}
		<div class="alert alert-error">
	    	<button data-dismiss="alert" class="close" type="button">×</button>
	        <span>{{ app.session.flash('error') }}</span>
	    </div>
	{% endif %}
{% endblock %}

{% block cuerpo %}	    

	<table class="table">
        <tbody>
        	<tr>
                <td><strong>No.</strong></td>
                <td>{{ imvpyp.id }}</td>
            </tr>
            <tr>
                <td><strong>Imv</strong></td>
                <td>{{ imvpyp.imv }}</td>
            </tr>
            <tr>
                <td><strong>Categoria_Pyp</strong></td>
                <td>{{ imvpyp.pyp }}</td>
            </tr>
            <tr>
                <td><strong>Edad_Inicial</strong></td>
                <td>{{ imvpyp.edadIni }}</td>
            </tr>
            <tr>
                <td><strong>Edad_Final</strong></td>
                <td>{{ imvpyp.edadFin }}</td>
            </tr>
            
            <tr>
                <td><strong>Sexo</strong></td>
                <td>{{ imvpyp.sexo }}</td>
            </tr>
            
        </tbody>
    </table>

	<div class="btn-group">
	    <a href="{{ path('imvpyp_edit', {'imvpyp':imvpyp.id}) }}" class="btn btn-primary">Modificar</a>
	    <a href="{{ path('imvpyp_list') }}" class="btn">Volver</a>
    </div>
{% endblock %}
